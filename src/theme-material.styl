.ag-clarity
    headerColor = #1D7498;
    headerBorder = 1px solid rgba(0, 0, 0, .2);
    menuWidth = 26px;
    cellSidePadding = 14px;
    cellTopPadding = 12px;

    box-shadow: 0 1px 3px rgba(0, 0, 0, .15);

    .ag-cell
        padding: cellTopPadding cellSidePadding cellTopPadding cellSidePadding;

    .ag-column-moving .ag-cell
        transition: left 0.2s;

    .ag-cell:focus
        //outline: none;

    .ag-cell-focus
        border: 1px solid darkgrey;

    .ag-cell-no-focus
        border-right: 1px solid rgba(0, 0, 0, .05);
        border-top: 1px solid transparent;
        border-left: 1px solid transparent;
        border-bottom: 1px solid transparent;

    .ag-cell-first-right-pinned
        border-left: 1px dotted grey;

    .ag-header
        background: headerColor;
        color: #FFF;
        font-weight: semi-bold;

    .ag-no-scrolls .ag-header-container
        border-bottom: headerBorder;

    .ag-header-cell
        position: relative;
        border-right: headerBorder;
        text-align: left;

    .ag-header-cell-moving .ag-header-cell-label
        opacity: 0.0;

    .ag-header-cell-moving
        background-color rgb(190,190,190);

    .ag-header-cell-moving-clone
        border-right: 1px solid grey;
        border-left: 1px solid grey;
        background-color:rgba(220, 220, 220, 0.8);

    .ag-header-group-cell
        border-right: headerBorder;

    .ag-header-group-cell-with-group
        border-bottom: headerBorder;

    .ag-header-cell-label
        padding: cellTopPadding menuWidth cellTopPadding cellSidePadding;
        cursor: pointer;

    .ag-header-cell-text
        padding-left: 2px;

    .ag-header-group-cell-label
        padding: 12px menuWidth 12px 12px;

    .ag-header-group-text
        margin-right: 2px;

    .ag-header-cell-menu-button
        position: absolute;
        right: 0;
        top: 0;
        bottom: 0;
        width: menuWidth;
        padding: 2px;
        cursor: pointer;
        background: rgba(0, 0, 0, .08);
        svg
            position: absolute;
            top: 17px;
            left: 7px;
            fill: #FFF;

    .ag-header-expand-icon
        cursor: pointer;

    .ag-pinned-right-header
        border-left: 1px solid grey;

    .ag-header-cell-menu-button:hover

    .ag-header-icon
        color: rgba(255, 255, 255, 0.75);

    .ag-row-selected
        background-color: powderblue !important;

    .ag-row
        border-bottom: 1px solid rgba(0, 0, 0, .15);

    .ag-overlay-loading-wrapper
        background-color: rgba(255, 255, 255, 0.5);

    .ag-overlay-loading-center
        background-color: #ffffff;
        border: 1px solid darkgray;
        border-radius: 10px;
        padding: 10px;

    .ag-overlay-no-rows-center
        background-color: #ffffff;
        border: 1px solid darkgray;
        border-radius: 10px;
        padding: 10px;

    .ag-body
        background-color: #fff;

    .ag-group-cell-entire-row
        background-color: #aaa;
        padding: 4px;

    .ag-footer-cell-entire-row
        background-color: #aaa;
        padding: 4px;

    .ag-group-cell
        font-style: italic;

    .ag-group-expand
        padding-right 2px;

    .ag-footer-cell
        font-style: italic;

    .ag-filter-checkbox
        position: relative;
        top: 2px;
        left: 2px;

    .ag-filter-header-container
        border-bottom: 1px solid lightgrey;

    .ag-filter-apply-panel
        border-top: 1px solid lightgrey;
        padding: 2px;

    .ag-filter
        border: 1px solid black;
        background-color: #f0f0f0;

    .ag-filter-value
        margin-left: 4px;

    .ag-selection-checkbox
        margin: 0px 12px 0 0;

    .ag-paging-panel
        padding: 4px;

    .ag-paging-button
        margin-left: 4px;
        margin-right: 4px;

    .ag-paging-row-summary-panel
        display: inline-block;
        width: 300px;

    .ag-column-not-visible
        opacity: 0.75;

    .ag-not-dragging
        border: 1px solid transparent;

    .ag-drop-target-above
        //border: 1px solid transparent;
        border-top: 5px solid darkgrey;

    .ag-drop-target-below
        //border: 1px solid transparent;
        border-bottom: 5px solid darkgrey;

    .ag-dragging
        border: 1px dotted darkgrey;

    .ag-list-item-selected
        color #f0f0f0;

    .ag-list-item-not-selected
        font-style italic;
        color #a0a0a0;

    .ag-tool-panel-container
        background-color: #eee;
        box-sizing: border-box;
        border: 1px solid darkgrey;
        padding: 4px;

    .ag-list-selection
        background-color: white;
        border: 1px solid darkgrey;
        box-sizing: border-box;

    .ag-popup-list .ag-list-item:hover
        background-color: lightblue;

    .ag-visible-icons
        padding-left: 2px;
        padding-right: 2px;
